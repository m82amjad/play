            $ItemName 			= $this->session->userdata('ItemName'); //Item Name
            $ItemPrice 			= $this->session->userdata('ItemPrice'); //Item Name
            $ItemNumber 			= $this->session->userdata('ItemNumber'); //Item Name
            $ItemDesc 			= $this->session->userdata('ItemDesc'); //Item Name
            $ItemQty 			= $this->session->userdata('ItemQty'); //Item Name
            $ItemTotalPrice 			= $this->session->userdata('ItemTotalPrice'); //Item Name
            $TotalTaxAmount 			= $this->session->userdata('TotalTaxAmount'); //Item Name
            $HandalingCost 			= $this->session->userdata('HandalingCost'); //Item Name
            $InsuranceCost 			= $this->session->userdata('InsuranceCost'); //Item Name
            $ShippinDiscount 			= $this->session->userdata('ShippinDiscount'); //Item Name
            $ShippinCost 			= $this->session->userdata('ShippinCost'); //Item Name
            $GrandTotal 			= $this->session->userdata('GrandTotal'); //Item Name